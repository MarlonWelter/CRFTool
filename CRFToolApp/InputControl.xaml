<UserControl x:Class="CRFToolApp.InputControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CRFToolApp"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="400">
    <StackPanel>
        <Button Name="InputB" Height="200" Background="LightYellow" FontSize="32" Click="InputB_Click">General</Button>
        <Button Name="SoftwareB" Height="200" Background="LightSteelBlue" FontSize="32" Click="SoftwareB_Click">Software Graph</Button>
        <Button Name="ProteinB" Height="200" Background="LightGreen" FontSize="32" Click="ProteinB_Click">Protein Graph</Button>
    </StackPanel>
</UserControl>
